{
  "id": "/live/mobile-banking",
  "cmd": "env | tee  /tmp/env.txt && python -m SimpleHTTPServer $PORT",
  "env": {
    "APP_VERSION":"3.2.4",
    "backend1_port":"18443",
    "backend1_host":"bk1-acpt.example.com",
    "timeout1":"2000",
    "timeout2":"2002",
    "prop4711":"whatever"
  },
  "instances": 1,
  "cpus": 0.05,
  "mem": 16.0,
  "disk": 10.0,
  "ports": [10000],
  "requirePorts": false,
  "backoffSeconds": 1,
  "backoffFactor": 1.15,
  "maxLaunchDelaySeconds": 300,
  "constraints": [["stage","LIKE","prod"],["hostname", "UNIQUE"]],
  "healthChecks": [
    {
      "protocol": "HTTP",
      "path": "/",
      "gracePeriodSeconds": 3,
      "intervalSeconds": 10,
      "portIndex": 0,
      "timeoutSeconds": 4,
      "maxConsecutiveFailures": 3
    }
  ],
  "dependencies": [],
  "upgradeStrategy": {
    "minimumHealthCapacity": 0,
    "maximumOverCapacity": 1
  }
}
